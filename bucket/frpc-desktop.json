    "homepage": "https://jwinks.com/p/frp/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/luckjiawei/frpc-desktop/releases/download/v1.2.3/Frpc-Desktop-Setup-1.2.3.exe#/dl.7z",
            "hash": "sha512:3bc2d9f805ad1d05e392bcbb5c2b15811ce0cbe43327bb0ac11090390a691a2e3e63a2441668f3af1bcd93a873617a96bd1bc7fa5e299135f7960487089b4f2c",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Frpc-Desktop.exe",
            "Frpc-Desktop",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": {
        "github": "https://github.com/luckjiawei/frpc-desktop"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/luckjiawei/frpc-desktop/releases/download/v$version/Frpc-Desktop-Setup-$version.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "$baseurl/latest.yml",
            "regex": "sha512:\\s+$base64"
        }
    }
}
