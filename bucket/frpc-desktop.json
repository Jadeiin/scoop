    "homepage": "https://jwinks.com/p/frp/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/luckjiawei/frpc-desktop/releases/download/v1.2.4/Frpc-Desktop-Setup-1.2.4.exe#/dl.7z",
            "hash": "sha512:a4eeec9a1c59ad8ae77dc66df6c23d4c145ccd8803f0b90bf9d9391540380ba40b2f49b579013c5d38d089ee66bdd09c9de7cc0d578e396105253c51ca34892d",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Frpc-Desktop.exe",
            "Frpc-Desktop",
            "--user-data-dir=\"$dir\\User Data\""
