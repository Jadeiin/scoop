    "architecture": {
        "64bit": {
            "url": "https://github.com/toeverything/AFFiNE/releases/download/v0.16.0/affine-stable-windows-x64.exe#/dl.7z",
            "hash": "sha512:3dc3e345218ebf07d70be03b54ce2b78d09464d3a3f94c813003a90e6c7600af9190f3b49154c1adccee5069cf7555e44ef9b629eba85007c8d09adba5f15e03",
            "pre_install": [
                "Remove-Item \"$dir\\*\" -Exclude 'AFFiNE-*-full.nupkg'",
